10 printchr$(147)
20 print "enter a number":input n
30 dim a$(n) 
40 def fn mod(m)=x-int(x/m)*m
50 for x=1 to n
60 m=3
70 a=fn mod(m) 
80 m=5
90 b=fn mod(m)
100 if a=0 and b=0 then a$(x)="fizzbuzz"
110 if a=0 and b<>0 then a$(x)="fizz"
120 if a<>0 and b=0 then a$(x)="buzz"
130 if a<>0 and b<>0 then a$(x)=str$(x)
140 next x
150 for y=1 to n
160 print a$(y),
170 next y
